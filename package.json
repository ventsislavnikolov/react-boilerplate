{
  "name": "react-boilerplate",
  "private": true,
  "version": "1.0.0",
  "author": {
    "name": "Ventsislav Nikolov"
  },
  "homepage": "https://github.com/ventsislavnikolov/react-boilerplate",
  "bugs": {
    "url": "https://github.com/ventsislavnikolov/react-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ventsislavnikolov/react-boilerplate.git"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest watch",
    "test:ci": "vitest run",
    "preview": "vite preview",
    "prepare": "husky install",
    "build": "tsc && vite build",
    "test:ui": "npx vitest --ui",
    "e2e:ui": "playwright test --ui",
    "lint-staged": "pnpx lint-staged",
    "e2e:headless": "playwright test",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "npx test-storybook",
    "test:coverage": "vitest run --coverage",
    "lint:fix": "eslint ./src/**/*.tsx --fix",
    "commitlint": "npx --no -- commitlint --edit",
    "prettier": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"./src/**/*.{ts,tsx,md}\"",
    "analyze": "npx vite-bundle-visualizer -o stats.html",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "coupling-graph": "npx madge --extensions ts,tsx,css,md,mdx ./ --exclude 'vite.config.js|test-setup.js|tailwind.config.js|release.config.js|README.md|prettier.config.js|postcss.config.js|playwright.config.js|pnpm-lock.yaml|lint-staged.config.js|jsconfig.json|commitlint.config.js|e2e/|dist|.husky|.github|node_modules|README.md|.storybook/|.eslintrc.js' --image graph.svg"
  },
  "engines": {
    "node": ">=18.x.x",
    "npm": ">=8.x.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@radix-ui/colors": "^1.0.1",
    "@radix-ui/react-toast": "^1.1.4",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/react": "^7.61.0",
    "@t3-oss/env-core": "^0.6.0",
    "i18next": "^23.4.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "react": "^18.2.0",
    "react-cva": "^1.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.3",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.14.2",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@commitlint/types": "^17.4.4",
    "@playwright/test": "^1.36.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.4",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "@storybook/addon-a11y": "^7.2.0",
    "@storybook/addon-coverage": "^0.0.9",
    "@storybook/addon-essentials": "7.2.0",
    "@storybook/addon-interactions": "7.2.0",
    "@storybook/addon-links": "7.2.0",
    "@storybook/addon-storysource": "^7.2.0",
    "@storybook/addon-styling": "^1.3.5",
    "@storybook/blocks": "7.2.0",
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "7.2.0",
    "@storybook/react-vite": "7.2.0",
    "@storybook/test-runner": "^0.12.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-istanbul": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "autoprefixer": "^10.4.14",
    "chromatic": "^6.20.0",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.9.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "react-test-renderer": "^18.2.0",
    "semantic-release": "^21.0.7",
    "semantic-release-ado": "^1.4.0",
    "storybook": "7.2.0",
    "storybook-addon-designs": "6.3.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-bundle-visualizer": "^0.10.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.33.0"
  }
}