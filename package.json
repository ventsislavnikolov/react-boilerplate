{
  "name": "react-boilerplate",
  "private": true,
  "version": "1.0.0",
  "author": {
    "name": "Ventsislav Nikolov"
  },
  "homepage": "https://github.com/ventsislavnikolov/react-boilerplate",
  "bugs": {
    "url": "https://github.com/ventsislavnikolov/react-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ventsislavnikolov/react-boilerplate.git"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest watch",
    "test:ci": "vitest run",
    "preview": "vite preview",
    "prepare": "husky install",
    "build": "tsc && vite build",
    "test:ui": "npx vitest --ui",
    "e2e:ui": "playwright test --ui",
    "lint-staged": "pnpx lint-staged",
    "e2e:headless": "playwright test",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "npx test-storybook",
    "test:coverage": "vitest run --coverage",
    "lint:fix": "eslint ./src/**/*.tsx --fix",
    "commitlint": "npx --no -- commitlint --edit",
    "prettier": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"./src/**/*.{ts,tsx,md}\"",
    "analyze": "npx vite-bundle-visualizer -o stats.html",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "coupling-graph": "npx madge --extensions ts,tsx,css,md,mdx ./ --exclude 'vite.config.js|test-setup.js|tailwind.config.js|release.config.js|README.md|prettier.config.js|postcss.config.js|playwright.config.js|pnpm-lock.yaml|lint-staged.config.js|jsconfig.json|commitlint.config.js|e2e/|dist|.husky|.github|node_modules|README.md|.storybook/|.eslintrc.js' --image graph.svg"
  },
  "engines": {
    "node": ">=18.x.x",
    "npm": ">=8.x.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@radix-ui/colors": "3.0.0",
    "@radix-ui/react-toast": "1.1.5",
    "@reduxjs/toolkit": "1.9.7",
    "@sentry/react": "7.74.0",
    "@t3-oss/env-core": "0.7.0",
    "i18next": "23.5.1",
    "i18next-browser-languagedetector": "7.1.0",
    "react": "18.2.0",
    "react-cva": "1.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "13.3.0",
    "react-redux": "8.1.3",
    "react-router-dom": "6.16.0",
    "tailwind-merge": "1.14.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.0",
    "@commitlint/config-conventional": "17.8.0",
    "@commitlint/types": "17.4.4",
    "@playwright/test": "1.39.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "11.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.2.1",
    "@semantic-release/npm": "11.0.0",
    "@semantic-release/release-notes-generator": "12.0.0",
    "@storybook/addon-a11y": "7.4.6",
    "@storybook/addon-coverage": "0.0.9",
    "@storybook/addon-essentials": "7.4.6",
    "@storybook/addon-interactions": "7.4.6",
    "@storybook/addon-links": "7.4.6",
    "@storybook/addon-storysource": "7.4.6",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/blocks": "7.4.6",
    "@storybook/jest": "0.2.3",
    "@storybook/react": "7.4.6",
    "@storybook/react-vite": "7.4.6",
    "@storybook/test-runner": "0.13.0",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/dom": "9.3.3",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.5.5",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "@vitejs/plugin-react": "4.1.0",
    "@vitest/coverage-istanbul": "0.34.6",
    "@vitest/ui": "0.34.6",
    "autoprefixer": "10.4.16",
    "chromatic": "7.4.0",
    "eslint": "8.51.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-testing-library": "6.1.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jsdom": "22.1.0",
    "lint-staged": "15.0.0",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "react-test-renderer": "18.2.0",
    "semantic-release": "22.0.5",
    "semantic-release-ado": "1.4.0",
    "storybook": "7.4.6",
    "storybook-addon-designs": "6.3.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "vite": "4.4.11",
    "vite-bundle-visualizer": "0.10.0",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "0.34.6"
  }
}
