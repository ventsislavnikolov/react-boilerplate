{
  "name": "react-boilerplate",
  "private": true,
  "version": "1.0.0",
  "author": {
    "name": "Ventsislav Nikolov"
  },
  "homepage": "https://github.com/ventsislavnikolov/react-boilerplate",
  "bugs": {
    "url": "https://github.com/ventsislavnikolov/react-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ventsislavnikolov/react-boilerplate.git"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest watch",
    "test:ci": "vitest run",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "test:ui": "npx vitest --ui",
    "e2e:ui": "playwright test --ui",
    "lint-staged": "pnpx lint-staged",
    "e2e:headless": "playwright test",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "npx test-storybook",
    "test:coverage": "vitest run --coverage",
    "lint:fix": "eslint ./src/**/*.jsx --fix",
    "commitlint": "npx --no -- commitlint --edit",
    "prettier": "prettier --check \"**/*.{js,jsx}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx}\"",
    "analyze": "npx vite-bundle-visualizer -o stats.html",
    "chromatic": "npx chromatic --project-token=chpt_7022130e8c5c901",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "coupling-graph": "npx madge --extensions js,jsx,css,md,mdx ./ --exclude 'vite.config.js|test-setup.js|tailwind.config.js|release.config.js|README.md|prettier.config.js|postcss.config.js|playwright.config.js|pnpm-lock.yaml|lint-staged.config.js|jsconfig.json|commitlint.config.js|e2e/|dist|.husky|.github|node_modules|README.md|.storybook/|.eslintrc.js' --image graph.svg"
  },
  "engines": {
    "node": ">=18.*.*",
    "npm": ">=9.*.*"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-toast": "^1.1.4",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/react": "^7.55.2",
    "i18next": "^22.5.1",
    "i18next-browser-languagedetector": "^7.0.2",
    "react": "^18.2.0",
    "react-cva": "^1.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.12.1",
    "tailwind-merge": "^1.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@playwright/test": "^1.35.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.3",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.3",
    "@storybook/addon-a11y": "^7.0.21",
    "@storybook/addon-essentials": "7.0.21",
    "@storybook/addon-highlight": "^7.0.21",
    "@storybook/addon-interactions": "7.0.21",
    "@storybook/addon-links": "7.0.21",
    "@storybook/addon-storysource": "^7.0.21",
    "@storybook/blocks": "7.0.21",
    "@storybook/react": "7.0.21",
    "@storybook/react-vite": "7.0.21",
    "@storybook/test-runner": "^0.11.0",
    "@storybook/testing-library": "0.0.14-next.2",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-istanbul": "^0.32.0",
    "@vitest/ui": "^0.32.1",
    "autoprefixer": "^10.4.14",
    "chromatic": "^6.19.8",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "semantic-release": "^21.0.5",
    "semantic-release-ado": "^1.4.0",
    "storybook": "7.0.21",
    "storybook-addon-designs": "7.0.0-beta.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-bundle-visualizer": "^0.7.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.32.0"
  }
}
